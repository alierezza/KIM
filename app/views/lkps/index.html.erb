<%= link_to "back", dashboards_path %></br>


LIST OF LKP
<table border=1>
<thead>
	<th>Tanggal kejadian</th>
	<th>Jam Kejadian</th>
	<th>Lokasi Kejadian</th>
	<th>Bentuk Kejadian</th>
	<th>Tanggal Pembuatan LKP</th>
	<th>Dibuat Oleh</th>
</thead>
<tbody>
	<% @lkps.each_with_index do |lkp| %>
		<tr>
			<td><%= link_to lkp.tanggal_kejadian, lkp %></td>
			<td><%= link_to lkp.jam_kejadian, lkp %></td>
			<td><%= link_to lkp.lokasi_kejadian, lkp %></td>
			<td><%= link_to lkp.bentuk_kejadian, lkp %></td>
			<td><%= link_to lkp.created_at, lkp %></td>
			<td><%= link_to lkp.user.nama, lkp %></td>
		</tr>
	<% end %>
</tbody>
</table>