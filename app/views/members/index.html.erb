<div class='inner-body'>
	<div class='panel panel-default'>
		<div class='panel-heading'>
			<h3 class="panel-title">List User</h3>
		</div>

		<div class='panel-body'>
			<table class="table">
				<thead>
					<th>Username</th>
					<th>Nama</th>
					<th>Alamat</th>
					<th>Kota / Kabupaten</th>
					<th colspan=2></th>
				</thead>
				<tbody>
					<% @users.each_with_index do |user| %>
					<tr>
						<td><%= user.email %></td>
						<td><%= user.nama %></td>
						<td><%= user.alamat %></td>
						<td><%= user.kota %></td>
						<td><%= link_to edit_member_path(user,:status=>true), class: 'btn btn-success btn-xs' do %><span class='glyphicon glyphicon-ok'></span> Approve<% end %></td>
						<td><%= link_to edit_member_path(user, :status=>false), class: 'btn btn-xs btn-danger' do %><span class='glyphicon glyphicon-remove'></span> Reject<% end %></td>
					</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>
