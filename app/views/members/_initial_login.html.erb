<div class='inner-body'>
	<div class='panel panel-default'>
		<div class='panel-heading'>
			<h3 class="panel-title">List Member</h3>

		</div>
		<div class='panel-body'>
		<%= form_tag members_path,:remote=>true, :method=>"GET" do %>
			<%= text_field_tag :nama,nil,:placeholder=>"Nama" %>
			<%= submit_tag "Search", class: "btn btn-default" %>
			<%= link_to "Show All", members_path, class: "btn btn-default", :remote => true %>
		<% end %>
			<div id="div_index_member" >
				<%= render "index_members" %>
			</div>
		</div>
	</div>
</div>